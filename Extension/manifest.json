{
    "manifest_version": 3,
    "name": "AtCoder Extension",
    "version": "1.0",
    "description": "Extension for AtCoder contests",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://atcoder.jp/contests/*/tasks"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }